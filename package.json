    "eslint": "^5.16.0",
    "json-loader": "^0.5.7",
    "mocha": "^6.1.4",
    "raw-loader": "^2.0.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
