{
  "name": "blacklistr",
  "version": "1.0.0",
  "description": "A website for visualizing country specific data",
  "scripts": {
    "start": "npx webpack-dev-server",
    "build": "python assets/boundaries/josm/exportJosmBoundariesToGeoJson.py && node js/build.js && npm run minify && webpack",
